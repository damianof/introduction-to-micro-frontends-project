  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@builtwithjavascript/vue-file-upload-tailwind": "^1.0.2",
    "@types/node": "^20.12.7",
    "@vitejs/plugin-vue": "^5.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.6"
  }
}
